  xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" version="1.1"
  width="720" height="900" viewBox="0 0 720 900">
  {{#icons}}
    <g id="mdi-{{name}}" transform="translate({{x}} {{y}}) scale(1.5)" fill="#555">
      <path d="{{path}}" />
